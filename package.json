{
    "name": "@ekaruztech/react-social-auth",
    "version": "0.0.0",
    "private": true,
    "workspaces": {
        "packages": [
            "package",
            "example"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nondefyde/react-social-auth"
    },
    "keywords": [
        "react social login",
        "social login",
        "react social oauth",
        "react social auth"
    ],
    "author": "nondefyde",
    "bugs": {
        "url": "https://github.com/nondefyde/react-social-auth/issues"
    },
    "homepage": "https://github.com/nondefyde/react-social-auth#readme",
